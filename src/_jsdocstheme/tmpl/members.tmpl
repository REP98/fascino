<?js
var data = obj;
var self = this;
?>
<h5 class="name" id="<?js= id ?>"><?js= data.attribs + name + (data.signature ? data.signature : '') ?></h5>

<?js if (data.summary) { ?>
<p class="summary"><?js= summary ?></p>
<?js } ?>

<?js if (data.description) { ?>
<div class="description">
	<?js= data.description ?>
</div>
<?js } ?>

<?js if (data.type && data.type.names) {?>
	<h5>Tipo: <?js= self.partial('type.tmpl', data.type.names) ?></h5>
<?js } ?>

<?js= this.partial('details.tmpl', data) ?>

<?js if (data.fires && fires.length) { ?>
	<h5>Dispara:</h5>
	<ul class="list-unstyled"><?js fires.forEach(function(f) { ?>
		<li><?js= self.linkto(f) ?></li>
	<?js }); ?></ul>
<?js } ?>

<?js if (data.examples && examples.length) { ?>
	<h5>Ejemplo<?js= examples.length > 1? 's':'' ?></h5>
	<?js= this.partial('examples.tmpl', examples) ?>
<?js } ?>
